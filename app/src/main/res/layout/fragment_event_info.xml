<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="java.util.ArrayList" />

        <variable
            name="items"
            type="ArrayList&lt;com.esspresso.nocnaukowcwpk.main.eventinfo.EventInfoItemModel>" />
    </data>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/question_category_list"
        itemLayout="@{@layout/item_schedule_event}"
        items="@{items}"
        variableName="@{`model`}"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layoutAnimation="@anim/recycler_item_fall_down"
        android:overScrollMode="never"
        android:scrollbars="none"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_name" />
</layout>
